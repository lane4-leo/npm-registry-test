{
  "name": "@lane4-leo/npm-registry-test",
  "version": "1.0.32",
  "description": "package example",
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prepublish": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js",
    "start:dev-backup": "nest start --watch",
    "start:dev": "NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:development-backup": "NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:development": "NODE_ENV=development node dist/src/main.js",
    "start:debug": "nest start --debug --watch",
    "start:produdction": "node dist/src/main.js",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:migrate": "npm run typeorm migration:run",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js src/config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "postinstall": "patch-package"
  },
  "author": "dabin",
  "license": "ISC",
  "keywords": [
    "console"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lane4-leo/npm-registry-test"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/testing": "^9.4.0",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.1.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  }
}
